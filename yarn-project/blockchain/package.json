{
  "name": "@aztec/blockchain",
  "version": "2.0.0",
  "license": "GPL-2.0-only",
  "exports": "./dest/index.js",
  "type": "module",
  "scripts": {
    "build": "yarn clean && tsc -b tsconfig.dest.json",
    "build:dev": "tsc -p tsconfig.dest.json --watch",
    "formatting": "run -T prettier --check ./src && run -T eslint --max-warnings 0 ./src",
    "clean": "rm -rf ./dest .tsbuildinfo",
    "start:ganache": "ganache-cli -h :: -p=8544 -l 12000000 -e 10000 -m 'test test test test test test test test test test test junk'",
    "start:ganache:fork": "ganache-cli -f https://mainnet.infura.io/v3/6a04b7c89c5b421faefde663f787aa35 --chain.chainId 0xe2e --fork.blockNumber 15918000 -h :: -p=8544 -l 15000000 -e 10000 -m 'test test test test test test test test test test test junk'",
    "test": "NODE_NO_WARNINGS=1 node --openssl-legacy-provider --experimental-vm-modules $(yarn bin jest) --no-cache --silent",
    "blockchain_cli": "node ./dest/cli/index.js"
  },
  "bin": {
    "blockchain_cli": "./dest/cli/index.js"
  },
  "jest": {
    "preset": "ts-jest/presets/default-esm",
    "globals": {
      "ts-jest": {
        "useESM": true
      }
    },
    "moduleNameMapper": {
      "^(\\.{1,2}/.*)\\.js$": "$1"
    },
    "testRegex": ".*\\.test\\.ts$",
    "rootDir": "./src",
    "testTimeout": 120000,
    "slowTestThreshold": 60
  },
  "dependencies": {
    "@aztec/barretenberg": "workspace:^",
    "@ethersproject/abi": "5.5.0",
    "@ethersproject/abstract-provider": "5.5.1",
    "@ethersproject/contracts": "5.5.0",
    "@ethersproject/experimental": "5.5.0",
    "@ethersproject/providers": "5.5.1",
    "@uniswap/v2-core": "^1.0.1",
    "@uniswap/v2-periphery": "^1.1.0-beta.0",
    "bip85": "^0.0.3",
    "commander": "^9.0.0",
    "cross-fetch": "^3.1.5",
    "debug": "^4.3.4",
    "ethers": "5.5.1",
    "sha3": "^2.1.4"
  },
  "devDependencies": {
    "@aztec/eslint-config": "workspace:^",
    "@jest/globals": "^29.3.1",
    "@jest/types": "^28.1.3",
    "@rushstack/eslint-patch": "^1.1.4",
    "@types/debug": "^4.1.7",
    "@types/jest": "^28.1.6",
    "@types/node": "^18.7.23",
    "ganache": "^7.5.0",
    "jest": "^28.1.3",
    "ts-jest": "^28.0.7",
    "ts-node": "^10.9.1",
    "tsc-watch": "^5.0.3",
    "typescript": "^4.7.4"
  }
}
