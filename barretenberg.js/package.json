{
  "name": "barretenberg",
  "version": "1.0.0",
  "main": "index.js",
  "license": "GPL-2.0",
  "scripts": {
    "build": "yarn clean && tsc -p tsconfig.cjs.json && tsc -p tsconfig.es.json && node ./package.js",
    "clean": "rm -rf ./dest ./dest-es",
    "test": "tsc && cp -R ./src/wasm/barretenberg.wasm ./dest/wasm && jest",
    "test:notes": "tsc && cp -R ./src/wasm/barretenberg.wasm ./dest/wasm && jest ./dest/client_proofs/note_algorithms/index.test.js",
    "test:sha": "tsc && cp -R ./src/wasm/barretenberg.wasm ./dest/wasm && jest ./dest/crypto/sha256/index.test.js",
    "build:dev": "yarn build:dev:cjs & yarn build:dev:es",
    "build:dev:cjs": "tsc --watch",
    "build:dev:es": "tsc -p tsconfig.es.json --watch",
    "symlink-wasm": "cp -R ./src/wasm/barretenberg.wasm ./dest/wasm && cp -R ./src/wasm/barretenberg.wasm ./dest-es/wasm"
  },
  "jest": {
    "rootDir": "dest",
    "testRegex": "dest/.*\\.test\\.js$",
    "moduleFileExtensions": [
      "ts",
      "js"
    ]
  },
  "dependencies": {
    "@types/levelup": "^4.3.0",
    "bigint-buffer": "^1.1.5",
    "debug": "^4.1.1",
    "detect-node": "^2.0.4",
    "fs-extra": "9.0.1",
    "levelup": "^4.4.0",
    "memdown": "^5.1.0",
    "node-fetch": "2.6.1",
    "promise-readable": "^6.0.0",
    "sha3": "^2.1.3",
    "threads": "1.3.1",
    "tslib": "^2.0.0"
  },
  "devDependencies": {
    "@types/debug": "^4.1.5",
    "@types/fs-extra": "9.0.1",
    "@types/jest": "^26.0.20",
    "@types/memdown": "^3.0.0",
    "@types/node": "^13.7.7",
    "@typescript-eslint/eslint-plugin": "^4.1.1",
    "@typescript-eslint/parser": "^4.1.1",
    "eslint": "^7.9.0",
    "eslint-config-prettier": "^6.11.0",
    "jest": "^26.6.3",
    "prettier": "^2.0.5",
    "typescript": "^3.8.3"
  }
}
