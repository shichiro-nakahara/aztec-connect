{
  "name": "barretenberg",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "build": "tsc -p tsconfig.cjs.json && cp ./src/wasm/barretenberg.wasm ./dest/wasm && cp package.json dest",
    "build:watch": "mkdir -p ./dest/wasm && cp -R ./src/wasm/barretenberg.wasm ./dest/wasm && tsc -p tsconfig.cjs.json --watch",
    "test": "jest",
    "start:dev": "tsc -p tsconfig.cjs.json --watch"
  },
  "jest": {
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testRegex": ".*\\.test\\.ts$",
    "moduleFileExtensions": [
      "ts",
      "js"
    ]
  },
  "dependencies": {
    "detect-node": "^2.0.4",
    "node-fetch": "^2.6.0",
    "threads": "^1.3.1"
  },
  "devDependencies": {
    "@types/jest": "^25.1.3",
    "@types/node": "^13.7.7",
    "jest": "^25.1.0",
    "ts-jest": "^25.2.1",
    "tslint": "^6.0.0",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^3.8.3"
  }
}
