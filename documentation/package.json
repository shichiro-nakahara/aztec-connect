    "@ethersproject/providers": "^5.0.5",
    "@ethersproject/units": "^5.0.2",
    "@microsoft/tsdoc": "^0.12.20",
    "@types/jest": "^26.0.13",
    "@types/react": "^16.9.46",
    "babel-loader": "^8.1.0",
    "clsx": "^1.1.1",
    "console-feed": "3.0.0",
