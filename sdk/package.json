{
  "name": "aztec2-sdk",
  "version": "2.0.0",
  "main": "index.js",
  "homepage": "https://developers.aztec.network",
  "author": "AZTEC",
  "license": "GPL-2.0",
  "scripts": {
    "build": "yarn clean && tsc -p tsconfig.cjs.json && tsc -p tsconfig.es.json && node ./package.js",
    "bundle": "tsc -p tsconfig.bundle.json && webpack --config ./webpack.prod.js && node ./package.bundle.js",
    "clean": "rm -rf ./dest ./dest-es ./dist",
    "test": "jest",
    "build:dev": "yarn build:dev:cjs & yarn build:dev:es",
    "build:dev:cjs": "tsc -p tsconfig.cjs.json --watch",
    "build:dev:es": "tsc -p tsconfig.es.json --watch"
  },
  "jest": {
    "transform": {
      "^.+\\.js$": "babel-jest",
      "^.+\\.tsx?$": "ts-jest"
    },
    "testRegex": ".*\\.test\\.ts$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ],
    "transformIgnorePatterns": [
      "/node_modules/(?!barretenberg).+\\.js$"
    ]
  },
  "dependencies": {
    "barretenberg": "file:../barretenberg.js/dest-es",
    "bigint-buffer": "^1.1.5",
    "blockchain": "file:../blockchain/dest-es",
    "broadcast-channel": "^3.1.0",
    "detect-node": "^2.0.4",
    "dexie": "^3.0.1",
    "ethers": "5.0.3",
    "idb-mutex": "^0.11.0",
    "level-js": "^5.0.2",
    "leveldown": "^5.6.0",
    "levelup": "^4.4.0",
    "sqlite3": "^5.0.0",
    "sriracha": "file:../sriracha/dest-es",
    "tslib": "^2.0.0",
    "typeorm": "^0.2.28"
  },
  "devDependencies": {
    "@babel/core": "^7.11.6",
    "@babel/plugin-proposal-class-properties": "^7.10.4",
    "@babel/plugin-proposal-decorators": "^7.10.5",
    "@babel/plugin-proposal-optional-chaining": "^7.11.0",
    "@babel/preset-env": "^7.11.5",
    "@babel/preset-typescript": "^7.10.4",
    "@types/jest": "^26.0.0",
    "@types/level-js": "^4.0.1",
    "@types/levelup": "^4.3.0",
    "@types/node": "^13.7.7",
    "@typescript-eslint/eslint-plugin": "^4.1.1",
    "@typescript-eslint/parser": "^4.1.1",
    "babel-loader": "^8.1.0",
    "babel-plugin-transform-typescript-metadata": "^0.3.0",
    "copy-webpack-plugin": "^6.1.1",
    "eslint": "^7.9.0",
    "eslint-config-prettier": "^6.11.0",
    "fake-indexeddb": "^3.0.2",
    "jest": "^25.1.0",
    "prettier": "^2.0.5",
    "terser-webpack-plugin": "^4.2.2",
    "threads-plugin": "^1.3.3",
    "ts-jest": "^25.2.1",
    "typescript": "^3.8.3",
    "webpack": "^4.44.2",
    "webpack-cli": "^3.3.12",
    "webpack-merge": "^5.1.4"
  }
}
