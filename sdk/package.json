{
  "name": "aztec2-sdk",
  "version": "1.0.0",
  "main": "index.js",
  "license": "GPL-2.0",
  "scripts": {
    "build": "yarn clean && tsc -p tsconfig.cjs.json && tsc -p tsconfig.es.json && node ./package.js",
    "clean": "rm -rf ./dest ./dest-es",
    "test": "jest",
    "build:dev": "yarn build:dev:cjs & yarn build:dev:es",
    "build:dev:cjs": "tsc -p tsconfig.cjs.json --watch",
    "build:dev:es": "tsc -p tsconfig.es.json --watch"
  },
  "jest": {
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testRegex": ".*\\.test\\.ts$",
    "moduleFileExtensions": [
      "ts",
      "js"
    ]
  },
  "dependencies": {
    "barretenberg": "file:../barretenberg.js/dest",
    "blockchain": "file:../blockchain/dest",
    "bigint-buffer": "^1.1.5",
    "broadcast-channel": "^3.1.0",
    "detect-node": "^2.0.4",
    "dexie": "^3.0.1",
    "ethers": "5.0.3",
    "idb-mutex": "^0.11.0",
    "level-js": "^5.0.2",
    "leveldown": "^5.6.0",
    "levelup": "^4.4.0",
    "sriracha": "file:../sriracha/dest",
    "tslib": "^2.0.0"
  },
  "devDependencies": {
    "@types/jest": "^26.0.0",
    "@types/level-js": "^4.0.1",
    "@types/levelup": "^4.3.0",
    "@types/node": "^13.7.7",
    "@typescript-eslint/eslint-plugin": "^4.1.1",
    "@typescript-eslint/parser": "^4.1.1",
    "eslint": "^7.9.0",
    "eslint-config-prettier": "^6.11.0",
    "fake-indexeddb": "^3.0.2",
    "jest": "^25.1.0",
    "prettier": "^2.0.5",
    "ts-jest": "^25.2.1",
    "typescript": "^3.8.3"
  }
}
