{
  "name": "@aztec/blockchain",
  "version": "2.0.0",
  "main": "index.js",
  "license": "GPLv2",
  "private": true,
  "scripts": {
    "build": "yarn clean && yarn compile && tsc -p tsconfig.dest.json && node ./package.js && chmod +x ./dest/deploy/index.js",
    "build:dev": "tsc -p tsconfig.dest.json --watch",
    "compile": "node ./ensure_versions && yarn hardhat compile",
    "clean": "rm -rf ./cache ./dest ./src/artifacts",
    "start:ganache": "yarn compile && ganache-cli -h 0.0.0.0 -l 12000000 -e 10000 -m 'test test test test test test test test test test test junk'",
    "start:ganache:fork": "yarn compile && ganache-cli -f https://mainnet.infura.io/v3/6a04b7c89c5b421faefde663f787aa35 --chain.chainId 0xA57EC --fork.blockNumber 14000000 --database.dbPath ./data -h 0.0.0.0 -l 15000000 -e 10000 -m 'test test test test test test test test test test test junk'",
    "test": "cd ./src/contracts/verifier/fixtures && ./create_fixture.sh true true && yarn compile && NODE_NO_WARNINGS=1 yarn jest",
    "deploy": "node ./dest/deploy/index.js",
    "ganache:tools": "node ./dest/ganache/index.js"
  },
  "bin": {
    "deploy_rollup_processor": "./deploy/index.js",
    "add_asset": "./deploy/add_asset/index.js"
  },
  "jest": {
    "transform": {
      "^.+\\.ts$": "ts-jest"
    },
    "testRegex": ".*\\.test\\.ts$",
    "rootDir": "./src",
    "testTimeout": 60000,
    "slowTestThreshold": 60
  },
  "dependencies": {
    "@aztec/barretenberg": "../barretenberg.js/dest",
    "@ethersproject/experimental": "5.5.0",
    "@nomiclabs/hardhat-ethers": "^2.0.2",
    "@nomiclabs/hardhat-etherscan": "^2.1.8",
    "@nomiclabs/hardhat-waffle": "^2.0.1",
    "@openzeppelin/contracts-upgradeable": "^4.4.2",
    "@uniswap/v2-core": "^1.0.1",
    "@uniswap/v2-periphery": "^1.1.0-beta.0",
    "commander": "^9.0.0",
    "debug": "^4.1.1",
    "dotenv": "^8.2.0",
    "ethers": "5.5.1",
    "hardhat": "^2.6.8",
    "hardhat-upgrades": "^0.0.0",
    "typescript": "^4.4.4"
  },
  "devDependencies": {
    "@openzeppelin/contracts": "4.4.0",
    "@types/debug": "^4.1.5",
    "@types/jest": "^26.0.15",
    "@types/mocha": "^9.0.0",
    "@typescript-eslint/eslint-plugin": "^4.1.1",
    "@typescript-eslint/parser": "^4.1.1",
    "crypto": "^1.0.1",
    "eslint": "^7.9.0",
    "eslint-config-prettier": "^6.11.0",
    "ethereumjs-util": "^7.0.7",
    "ganache": "^7.0.2",
    "jest": "^27.0.6",
    "prettier": "^2.2.1",
    "prettier-plugin-solidity": "^1.0.0-beta.3",
    "ts-jest": "^27.0.3",
    "ts-node": "^8.9.1",
    "tsc-watch": "^2.2.1"
  },
  "resolutions": {
    "ethers": "5.5.1"
  }
}
