{
  "name": "@aztec/blockchain",
  "version": "2.0.0",
  "main": "index.js",
  "license": "GPLv2",
  "private": true,
  "scripts": {
    "build": "yarn clean && yarn compile && tsc -p tsconfig.dest.json && node ./package.js && chmod +x ./dest/deploy/index.js",
    "build:dev": "tsc -p tsconfig.dest.json --watch",
    "compile": "node ./ensure_versions && yarn hardhat compile",
    "clean": "rm -rf ./cache ./dest",
    "test": "yarn test:src && yarn test:contract",
    "test:parallel": "mocha --require hardhat/register --recursive --parallel --exit --timeout 20000 ./test/**/*.test.ts",
    "test:contract": "node ./ensure_versions && hardhat test ./test/**/*.test.ts",
    "test:verifier": "hardhat test ./test/verifier/verifier.test.ts",
    "test:src": "jest",
    "start:ganache": "yarn compile && ganache-cli -d -h 0.0.0.0 --chainId=1337 -l 12000000 -e 10000 -m 'test test test test test test test test test test test junk'",
    "generate:dev": "./generate_vks_dev.sh && cd ./test/verifier/fixtures && ./create_proofs_dev.sh",
    "generate:prod": "./generate_vks_prod.sh && cd ./test/verifier/fixtures && ./create_proofs_prod.sh"
  },
  "bin": {
    "deploy_rollup_processor": "./deploy/index.js",
    "add_asset": "./deploy/add_asset/index.js"
  },
  "jest": {
    "transform": {
      "^.+\\.ts$": "ts-jest"
    },
    "testRegex": ".*\\.test\\.(tsx?|js)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ],
    "rootDir": "./src"
  },
  "dependencies": {
    "@aztec/barretenberg": "../barretenberg.js/dest",
    "@ethersproject/experimental": "5.0.9",
    "@ethersproject/providers": "5.0.18",
    "@uniswap/v2-core": "^1.0.1",
    "@uniswap/v2-periphery": "^1.1.0-beta.0",
    "debug": "^4.1.1",
    "dotenv": "^8.2.0",
    "ethers": "5.0.29",
    "typescript": "^3.5.3"
  },
  "devDependencies": {
    "@nomiclabs/hardhat-ethers": "^2.0.1",
    "@nomiclabs/hardhat-waffle": "^2.0.0",
    "@openzeppelin/contracts": "^3.3.0",
    "@types/debug": "^4.1.5",
    "@types/jest": "^26.0.15",
    "@types/mocha": "^8.0.3",
    "@typescript-eslint/eslint-plugin": "^4.1.1",
    "@typescript-eslint/parser": "^4.1.1",
    "chai": "^4.2.0",
    "crypto": "^1.0.1",
    "eslint": "^7.9.0",
    "eslint-config-prettier": "^6.11.0",
    "ethereum-waffle": "3.0.0",
    "ethereumjs-util": "^7.0.7",
    "hardhat": "^2.4.3",
    "jest": "^26.6.1",
    "mocha": "^9.0.2",
    "prettier": "^2.2.1",
    "prettier-plugin-solidity": "^1.0.0-beta.3",
    "sinon": "^9.0.2",
    "ts-jest": "^26.4.2",
    "ts-node": "^8.9.1",
    "tsc-watch": "^2.2.1"
  }
}
