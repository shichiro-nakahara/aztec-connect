{
  "name": "hummus",
  "version": "1.0.1",
  "main": "dist/main.js",
  "license": "GPL-2.0",
  "scripts": {
    "start": "serve -p 8080 ./dist",
    "build": "yarn clean && yarn formatting && yarn tsc && parcel build ./src/index.html --no-source-maps",
    "build:dev": "parcel ./src/index.html --port 8080 --hmr-port 41337 --no-source-maps",
    "test": "tsc",
    "formatting": "prettier --check ./src && eslint --max-warnings 0 ./src",
    "clean": "rm -rf ./dist ./cache"
  },
  "browserslist": [
    "last 1 chrome version"
  ],
  "dependencies": {
    "serve": "^13.0.2"
  },
  "devDependencies": {
    "@aztec/sdk": "file:../sdk/dest",
    "@types/cbor": "^5.0.0",
    "@types/debug": "^4.1.5",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "@types/styled-components": "^5.1.0",
    "@typescript-eslint/eslint-plugin": "^5.27.0",
    "@typescript-eslint/parser": "^5.27.0",
    "cbor": "^6.0.1",
    "copy-to-clipboard": "^3.3.1",
    "debug": "^4.1.1",
    "eslint": "^8.16.0",
    "eslint-config-prettier": "^8.5.0",
    "parcel-bundler": "^1.12.5",
    "prettier": "^2.0.5",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "styled-components": "^5.1.0",
    "typescript": "^4.4.4"
  },
  "resolutions": {
    "terser": "4.8.0"
  }
}
