{
  "name": "hummus",
  "version": "1.0.0",
  "main": "dist/main.js",
  "license": "GPL-2.0",
  "scripts": {
    "build": "webpack --config webpack.prod.js",
    "generate:styles": "guacamole generateStyles",
    "lint": "tslint './src/**/*.ts?(x)'",
    "start": "node ./server",
    "start:dev": "webpack-dev-server --config webpack.dev.js",
    "test": "jest"
  },
  "jest": {
    "transform": {
      "^.+\\.js$": "babel-jest",
      "^.+\\.tsx?$": "ts-jest"
    },
    "testRegex": ".*\\.test\\.ts$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ],
    "transformIgnorePatterns": [
      "/node_modules/(?!barretenberg-es).+\\.js$"
    ]
  },
  "dependencies": {
    "@aztec/guacamole-ui": "^1.2.1",
    "css-loader": "^3.5.2",
    "dexie": "^2.0.4",
    "koa": "^2.5.2",
    "koa-compress": "^3.0.0",
    "koa-static": "^5.0.0",
    "level-js": "^5.0.2",
    "levelup": "^4.4.0",
    "mini-css-extract-plugin": "^0.9.0",
    "react-copy-to-clipboard": "^5.0.2",
    "svg-sprite-loader": "^4.2.5"
  },
  "devDependencies": {
    "@babel/core": "^7.1.0",
    "@babel/preset-env": "^7.1.0",
    "@babel/types": "^7.9.0",
    "@types/cbor": "^5.0.0",
    "@types/debug": "^4.1.5",
    "@types/jest": "^23.3.1",
    "@types/koa": "^2.0.46",
    "@types/koa-static": "^4.0.0",
    "@types/level-js": "^4.0.1",
    "@types/levelup": "^4.3.0",
    "@types/react": "^16.9.23",
    "@types/react-copy-to-clipboard": "^4.3.0",
    "@types/react-dom": "^16.9.5",
    "@types/styled-components": "^5.1.0",
    "@types/webpack-env": "^1.13.6",
    "cbor": "^5.0.1",
    "debug": "^4.1.1",
    "file-loader": "^5.1.0",
    "html-webpack-plugin": "^3.2.0",
    "jest": "^23.5.0",
    "prettier": "^2.0.5",
    "react": "^16.13.0",
    "react-dom": "^16.13.0",
    "styled-components": "^5.1.0",
    "threads-plugin": "^1.3.1",
    "ts-jest": "^23.1.4",
    "ts-loader": "^4.4.2",
    "tslint": "^6.1.2",
    "tslint-config-prettier": "^1.15.0",
    "typescript": "^3.8.3",
    "webpack": "^4.16.0",
    "webpack-cli": "^3.0.8",
    "webpack-dev-server": "^3.1.4"
  },
  "resolutions": {
    "babel-core": "7.0.0-bridge.0"
  }
}
